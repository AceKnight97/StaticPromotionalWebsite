s/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Constants/apiKeys.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/constant.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Redux/Store/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Redux/Actions/reload.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Helpers/auth.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/Layout/chapter.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/PerfectCultivator/chapter1.js",["320","321","322","323","324","325","326","327","328","329","330","331","332","333","334","335","336","337","338","339","340","341","342","343","344","345","346","347","348","349","350","351","352"],"const Chapter1 = `\nChương 1: Tạo Hóa Vô Tình Hữu Ý.\n\nTrải dài hàng chục dặm ngoài rìa rừng là Hòa Sinh Trấn, một thôn trấn nho nhỏ với nhân khẩu không đến 500 người.\n\nDọc theo từng con đường đất thô sơ, cỏ mọc đầy, man mát từng cơn gió nhẹ, lại thêm khí trời đêm lành lạnh, khiến cho người ta chỉ muốn ở lại trong nhà, hưởng thụ cái sự bình yên và ấm cúng của gia đình.\n\nTại một địa phương dễ nhìn thấy nhất trong trấn nhỏ, phía sau dược viên của một hiệu thuốc cũ kỹ, lờ mờ ánh đèn cầy hiện lên mái ấm gia đình khiến cho bao người trong trấn phải ngưỡng mộ.\n\nBên trong gian nhà có một lão già đang ngồi ở một góc. Tuy mắt lão nhắm dưỡng thần nhưng lại không che đi sự hiền hòa phát ra từ bản chất. Tay đang nâng chén trà, như lại muốn bỏ xuống, tựa hồ đang chờ đợi điều gì.\n\nĐặt ở giữa gian nhà là một đại hán chừng ba mươi tuổi, khuôn mặt có mấy phần giống lão già. Đôi mắt nhìn chằm chằm xuống bàn, tâm tình có chút nôn nóng không thể chờ được, thêm một không ít sự kỳ vọng.\n\nNếu nhìn vào thì bất kì ai cũng có thể đoán được là cha con họ đang chờ đợi một điều gì đó vô cùng quan trọng.\n\nCăn phòng gần đó không ngừng phát ra âm thanh đau đớn, cùng thở gấp của một thiếu phụ, bên cạnh còn có những lời khích lệ “cố lên” có phần gấp gáp. \n\n\nHai người một già một trẻ. Rõ ràng là người đàn bà lớn tuổi là bà đỡ, đang giúp cho thiếu phụ ấy sinh con.\n\nBỗng một thân ảnh đột nhiên xuất hiện bên ngoài dược viên, không một dấu hiệu báo trước. \n\nThân ảnh vóc dáng thanh niên trẻ tuổi, một khuôn mặt khôi ngô, vẻ mặt ngoài sự điềm đạm còn có mong chờ và đề phòng. Quanh người lại không có một tia linh lực ba động, so với người bình thường không khác một điểm nào, thế nhưng không có một người bình thường nào lại có thể vô thanh vô tức xuất hiện như hắn.\n\nChần chừ một lát, hắn vẫn quyết định tiến vào căn nhà. Ngang nhiên bước vào, đi qua trước mặt lão già cùng tên thanh niên, thế nhưng chẳng ai thấy có gì bất thường. \n\nKhông sai, tên thanh niên trẻ tuổi này thân phận không hề bình thường, hắn dùng một loại thiên địa ẩn nấp công pháp che đi sự hiện diện của bản thân, không chừa một chút dấu vết.\n\n- Phụ thân, chúng ta có nên tiến vào xem một chút không? \n\nĐại hán mất kiên nhẫn hỏi.\n\nLão già nhẹ lắc đầu rồi nói:\n\n- Mẹ ngươi làm bà đỡ hơn 30 năm rồi, ngươi lại còn lo lắng cũng không làm thêm được gì. \n\nNgừng một tý, lão già lại nói:\n\n- Cứ đợi đi, mẹ ngươi có thể chiếu cố tốt được cho nương tử cùng con của ngươi thôi.\n\nĐại hán định nói thêm gì nhưng vẫn cố nuốt xuống.\n\n- Hóa ra đây là một nhà bốn người.\n\nNghĩ thầm trong lòng, thanh niên thần bí cảm thán, tâm có chút ngưỡng mộ vì hắn chưa bao giờ tận hưởng qua cái loại vui sướng hạnh phúc gia đình ấy.\n\n- Oa, oa, oa ….\n\nThanh âm chào đời cuộc của đứa trẻ vang lên, như thiên lôi đánh bay đi tất cả suy tư cùng trầm mặc.\n\n- Vào được rồi, nhanh lên, nhanh vào đây xem!\n\nLão bà vội vàng gọi, âm thanh chứa đầy quang hỷ.\n\nNghe tiếng gọi, hai người liền chạy vào. Thấy mẫu thân đang bế đứa bé, đại hán chạy ngay đến cạnh vợ, ân cần thăm hỏi.\n\nLão già cũng vô cùng vui sướng, nói liên tục ba, bốn chữ tốt. Không chỉ thế, lão còn cao hứng ngâm thơ:\n\nTích nhật ác xúc bất túc khoa.\nKim triêu phóng đãng tứ vô nhai.\n\nĐứa trẻ vô cùng khôi ngô, linh hoạt, tiền đô rộng mở.\n\nLúc này lão cũng thấy đứa bé này có chút đặc biệt. \n\n- Không đúng a, làn da đứa trẻ này như có như không phát ra một loại hào quang thì phải, các ngươi mau đến xem xem. \n\nTiếng khóc cũng dần nhỏ lại.\n\nKhi lão già cùng đại hán chạy đến, đã không còn nghe được tiếng khóc của đứa trẻ nữa.\n\n- Đúng rồi, sao trên thân nó lại phát ra thứ ánh sáng kia, đó là gì a?\n\nĐại hán lo âu hỏi.\n\n- Ta cũng không biết a. Ta chưa bao giờ thấy qua ai như vậy.\n\nLão già nghiêm nghị trả lời.\n\n- Có lẽ nào là linh vận. Không đúng a, chỉ có tiên nhân mới có linh vận. Nếu cháu ta mới sinh ra đã có linh vận, chẳng khác nào nó đã là tiên nhân.\n\nBà lão hay nghe ngóng về các câu chuyện, truyền thuyết trong dân gian đồn thổi, một ít thực hư không rõ, chỉ dám cho một cái giả thuyết. \n\nMọi người có chút rơi vào trầm mặc. \n\n- Mau, mau cho ta xem con của ta.\n\nNgười thiếu phụ lên tiếng cắt đứt trầm mặc. Chỉ trong thoáng chốc, mọi người lại bừng tĩnh, đứa trẻ này chắc chắn tương lai sẽ sáng rọi, cũng lấy đó làm vui mừng.\n\n- Sao hơi thở đứa trẻ lại yếu đến gần như không còn như thế? Nó đang hấp hối a.\n\nThanh niên thần bí đứng quan sát tất cả lên tiếng hỏi, cũng chẳng biết hắn đang hỏi ai.\n\n- Thưa chủ nhân, thân thể đứa trẻ này là Sát Thần Đạo Thể, đứng đầu trong các loại Đạo Thể. Sát Thần Đạo Thể hấp thụ linh khí cùng đạo vận quá mạnh mẽ, linh hồn bé nhỏ của nó không thể chịu đựng được, sẽ dần tiêu tán bên trong cơ thể.\n\nThanh âm từ thiên địa truyền đến, âm giọng như một tiểu cô nương.\n\n- Con của ta! Sao lại thế này? Sao con của ta lại không thở nữa?\n\nThiếu phụ hốt hoảng la lên. Mọi người cũng nhanh chóng đến xem.\n\nThanh niên thần bí nhìn thấy tình cảnh cả nhà bốn người bọn họ đang bất lực nhìn đứa trẻ vừa mới chào đời không bao lâu lại đang hấp hối. Dù cho có sắt đá cũng không thể làm ngơ, huống hồ hắn đây là một người đa sầu đa cảm.\n\n- Vô phương thôi! Nếu muốn cùng tồn tại với Sát Thần Đạo Thể, linh hồn phải thực sự mạnh mẽ. Ngươi có thể cưỡng ép làm cho linh hồn ấy mạnh lên, nhưng nó chỉ là một đứa trẻ mới sinh, sẽ không chịu nổi mà vẫn lạc.\n\nThiên địa thanh âm như biết thanh niên thần bí ý nghĩ, không đợi thanh niên thần bí hỏi, liền trả lời.\n\n- Aaaaa …! Ông trời ơi, ông có mắt hay không! Con ta có tội tình gì chứ!\n\nThiếu phụ vừa khóc vừa la lên trong tuyệt vọng. Bên cạnh lão bà cũng vừa quỳ lạy vừa kêu:\n\n- Tiên nhân, ông trời ơi! Xin hãy cứu giúp lấy gia đình của lão, cho dù bắt lão già này phải giảm thọ, lão cũng bằng lòng.\n\nLà trụ cột trong gia đình, thấy tình cảnh này, đại hán cũng đành tuyệt vọng, quỳ xuống giường, bàn tay cố che đi nước mắt đang chảy.\n\n- Ông trời, tiên nhân, tổ tiên à, ta cả đời hành y cứu người. Sao lại để ta đến cuối đời phải chịu cảnh kẻ đầu bạc tiễn kẻ đầu xanh như vậy a!\n\nLão già cũng không kiềm được, gục sang một bên than trách.\n\nTrầm mặc đã lâu thanh niên thần bí, cũng không kiếm nén được cảm xúc, hắn đã rơi lệ. Sau bấy nhiêu năm thời gian, đây là lần đầu hắn rơi lệ vì người khác, mà không phải vì mình. \n\nHắn nắm thật chặt hai bàn tay, ra quyết định nhất định phải cứu lấy đứa trẻ này, không thể để một gia đình đang êm ấm như thế bị tan nát, không thể để lão già phải chịu cảnh kẻ đầu bạc tiễn kẻ đầu xanh.\n\n- Tiểu tiểu, ta nhất định phải cứu lấy nó. Dù cho sau này có phải đối đầu với nó đi chăng nữa, ta hôm nay cũng NHẤT ĐỊNH PHẢI CỨU LẤY ĐỨA TRẺ NÀY.\n\nThanh niên thần bí như chém đinh chặt sắt nói, đôi mắt toát ra một sự quyết tâm không thể lay chuyển.\n\n- Nếu cưỡng ép làm cho linh hồn của nó mạnh lên chỉ là khiến nó chết đi nhanh hơn. Cũng không thể tách rời linh hồn ấy ra khỏi thân thể. Phàm là Đạo Thể, đều có mối liên hệ vô cùng chặt chẽ với linh hồn chủ, không thể tách ra. Đó là nguyên nhân khiến cho rất ít người có thể đoạt xá Đạo Thể của kẻ khác.\n\nNgừng một chút, thiên âm lại truyền đến:\n\n- Chi bằng chủ nhân hãy tiến vào thân thể này. \n\nChưa đợi thanh niên thần bí hỏi lại, thiên địa chi âm lại một lần nữa truyền đầy đủ thông tin cùng cách thức, lý giải rõ ràng tiền căn hậu quả.\n\nThanh niên thần bí quyết định dùng phương pháp mà thiên địa chi âm truyền đến. Tiến vào ở đây vốn là một loại phương thức đoạt xá, như đẳng cấp vô cùng cao. \n\nĐem cả thân thể và linh hồn của kẻ muốn đoạt xá vào bên trong đan điền của chủ thể. Đồng thời bắt giữ linh hồn của chủ thể cách ly ở một bên, có thể từ từ nuôi dưỡng. Chỉ cần linh hồn chủ thể còn ở tại thân thể, Đạo Thể sẽ không tiên tán cũng như bài xích.\n\nThanh niên thần bí vốn không thích phương thức này, nhưng nó lại là phương thức duy nhất có thể cứu lấy đứa trẻ cũng như cả nhà 4 người này. \n\nHắn không còn do dự, liền theo phương thức đạt được mà tiến hành đoạt xá. \n\nLinh hồn vốn sắp phải chết, không hề có một chút chống cự nào, liền ngoan ngoãn tụ lại, nằm trong một khoảng không gian mà thanh niên thần bí đã cẩn thận chuẩn bị.\n\nLúc này hắn cũng đang ở trong đan điền của đứa trẻ, cho linh hồn mình từ từ thoát ly thân thể tiến nhập ý thức của nó, làm chủ lấy thân thể này.\n\nĐứa bé bắt đầu hô hấp trở lại, mắt cũng mở to ra. \n\n- Nó tỉnh rồi!\n\nThiếu phụ thốt lên, thanh âm mang niềm vui bất ngờ.\n\nNhững người còn lại cũng nhanh chóng nhận ra, đứa trẻ mới vừa tưởng chừng đã chết, giờ lại mạnh mạnh, khỏe khỏe, nhìn thẳng vào từng người họ, nào có chút gì là suy nhược.\n\nTâm trạng cả nhà liền chuyển biến, ai cũng vui vẻ hớn hở cười, giờ đây những giọt nước mắt không mang một tia nỗi buồn, chỉ còn lại sự vui sướng cùng biết ơn.\n\n- Đa tạ ông trời, đa tạ tiên nhân, đa tạ tổ tiên a! Chúng ta đã thực sự trách làm các người, chúng ta ở đây nhận lỗi trước người, xin người đừng trách tội cho những kẻ phàm nhân vô vô tri này. \n\nLão già đi đầu vái lạy tứ phương. Tiếp theo sau là từng người, đến cả thiếu phụ cũng vội đưa con cho lão bà rồi vui vẻ vái lạy, thể hiện hối lỗi.\n\nThanh niên thần bí cũng biết, trong tương lai cũng sẽ có một ngày, hắn phải trả lại thân thể này cho linh hồn bé nhỏ kia, đó cũng có thể là ngày mà hắn phải đối đầu với nó.\n\nHết Chương 1: Tạo Hóa Vô Tình Hữu Ý.\n\n`;\nexport default Chapter1;\n","/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/PerfectCultivator/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/PerfectCultivator/chapter3.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/PerfectCultivator/chapter2.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Helpers/customHooks.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Helpers/fetch-client.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/LightNovel/PerfectCultivator/chapter4.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/App.js",[],["353","354","355","356","357","358"],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Redux/Store/localStorage.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Redux/Reducers/initialState.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Config/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/MyCV/Layout/cvInformation.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/MyCV/Layout/cvDetails.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/MyCV/index.js",["359"],"import React from 'react';\nimport { Button } from 'antd';\nimport { useHistory } from 'react-router-dom';\nimport { ArrowLeftOutlined, ArrowRightOutlined } from '@ant-design/icons';\nimport { useMergeState } from '../../Helpers/customHooks';\n\nimport CVInformation from './Layout/cvInformation';\nimport CVDetails from './Layout/cvDetails';\nimport Portfolio from './Portfolio';\nimport { PAGE_MANAGER } from '../../Constants';\n\nconst MyCV = () => {\n  const history = useHistory();\n  const [state, setState] = useMergeState({\n    isShowDetails: false,\n  });\n\n  const { isShowDetails } = state;\n\n  const onClickBack = () => {\n    if (isShowDetails) {\n      setState({ isShowDetails: false });\n      return;\n    }\n    // history.push(PAGE_MANAGER.HOME);\n    history.goBack();\n  };\n\n  const onClickDetailCV = () => {\n    setState({ isShowDetails: true });\n  };\n\n  return (\n    <div className='my-cv-wrapper'>\n      <div className='my-cv-main'>\n\n        {\n          isShowDetails\n            ? <Portfolio />\n            : (\n              <>\n                <CVInformation />\n                <CVDetails />\n              </>\n            )\n        }\n\n      </div>\n\n      <Button\n        type='ghost'\n        className='cv-btn-back'\n        onClick={onClickBack}\n      >\n        <ArrowLeftOutlined />\n        Back\n      </Button>\n\n      {\n        !isShowDetails && (\n          <Button\n            type='ghost'\n            className='cv-btn-details'\n            onClick={onClickDetailCV}\n            // icon={}\n          >\n            More informtion\n            <ArrowRightOutlined />\n          </Button>\n        )\n      }\n    </div>\n  );\n};\nMyCV.defaultProps = {\n};\nMyCV.propTypes = {\n};\nexport default MyCV;\n","/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/MyCV/Portfolio/index.js",[],"/Users/triettruong/Documents/StaticPromotionalWebsite/src/Pages/MyCV/Layout/bodyTopic.js",["360"],"import React from 'react';\nimport PropTypes from 'prop-types';\nimport classnames from 'classnames';\nimport _ from 'lodash';\nimport { } from 'antd';\n\nconst BodyTopic = (props) => {\n  const { className, title } = props;\n  return (\n    <div className={classnames('body-topic-wrapper', className)}>\n      <div className='summary'>\n        <span>{title.toUpperCase()}</span>\n      </div>\n      <div className='border-line' />\n    </div>\n  );\n};\nBodyTopic.defaultProps = {\n  className: '',\n  title: '